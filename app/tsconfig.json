{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "lib": [
            "es2015",
            "es6",
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strict": true,
        "module": "commonjs",
        "target": "es5",
        "esModuleInterop": true,
        "jsx": "react",
    }
}